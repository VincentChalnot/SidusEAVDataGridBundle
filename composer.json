{
    "name": "sidus/eav-datagrid-bundle",
    "description": "Extension for sidus/eav-model-bundle, sidus/datagrid-bundle and sidus/eav-filter-bundle to allow datagrid management with EAV model",
    "keywords": [
        "eav",
        "datagrid"
    ],
    "homepage": "https://github.com/VincentChalnot/SidusEAVDataGridBundle",
    "type": "library",
    "license": "GPL-2.0",
    "authors": [
        {
            "name": "Vincent Chalnot",
            "email": "vincent@sidus.fr",
            "homepage": "http://chalnot.fr",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "Sidus\\EAVDataGridBundle\\": ""
        }
    },
    "require": {
        "sidus/eav-model-bundle": "1.2.*",
        "sidus/eav-filter-bundle": "1.2.*",
        "sidus/datagrid-bundle": "1.2.*"
    }
}
